.navbar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  max-width: 1320px;
  padding: 1.2rem 0;
  margin: 0 auto; }

.nav-links {
  display: flex;
  align-items: baseline;
  justify-content: center; }
  .nav-links .logo {
    font-size: 1.6rem !important;
    font-weight: bold;
    color: var(--accent);
    margin-right: 0.5rem; }
  .nav-links .menu {
    margin: 0 0.25em;
    font-size: 1.2rem;
    padding: 0 0.25rem; }
    .nav-links .menu:hover {
      border-bottom: 2px solid var(--accent); }

.icons a {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  padding: 0.25rem 0.5rem;
  border-radius: 5px;
  font-size: 1.2rem; }
  .icons a:hover {
    background-color: var(--dark); }
  .icons a svg {
    width: 1.6rem;
    height: 1.6rem;
    stroke: var(--accent); }

.intro {
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-top: 4rem; }

.intro-text h1 {
  font-size: 4rem;
  text-align: center;
  margin: 0; }

.intro-text span {
  color: var(--accent); }

.intro-text p {
  font-size: 1.4rem;
  text-align: center; }

.intro-links {
  margin: 0 auto; }
  .intro-links a {
    padding: 0.5rem 1rem;
    background-color: var(--accent);
    border-radius: 5px;
    font-size: 1.2rem; }

.articles h2 {
  font-size: 2rem;
  margin: 0.25em 0; }

.articles .posts a {
  display: flex;
  align-items: center;
  gap: 0.5em;
  padding: 0.5em; }
  .articles .posts a:hover {
    background-color: var(--tr-dark);
    border-radius: 5px; }
  .articles .posts a time {
    font-size: 1rem;
    font-weight: bold;
    color: var(--accent);
    margin-right: 0.5em; }
  .articles .posts a .post-title {
    font-size: 1.2rem;
    flex-grow: 2; }
  .articles .posts a span {
    font-size: 0.8rem;
    padding: 0.25rem 0.5rem;
    border-radius: 0.25rem;
    background-color: var(--accent); }

.projects h2 {
  font-size: 2rem;
  margin: 0.25em 0; }

.article-header {
  margin: 4rem 0 2rem; }
  .article-header h1 {
    font-weight: bold;
    font-size: 3rem;
    margin: 0; }

.post-meta .metadata {
  margin-bottom: 0.5rem; }

.post-meta .tags {
  display: flex;
  gap: 0.5rem; }
  .post-meta .tags a {
    font-size: 0.9rem;
    background-color: var(--dark);
    border-radius: 0.2rem;
    padding: 0.2rem 0.4rem; }
    .post-meta .tags a:hover {
      background-color: var(--accent); }

.article-post a {
  color: var(--accent); }
  .article-post a:hover {
    text-decoration: underline; }

.suggested {
  display: flex;
  justify-content: space-between; }
  .suggested a {
    flex: 0 0 calc(50% - 1rem);
    padding: 1rem;
    border-radius: .35rem;
    background: var(--dark);
    color: var(--accent);
    transition: transform 0.2s; }
    .suggested a:first-of-type {
      text-align: right; }
    .suggested a span {
      display: block;
      color: var(--light); }
    .suggested a:hover {
      transform: translateY(-4px); }

.section-title h1 {
  font-size: 3rem;
  margin: 2rem 0 0rem;
  text-align: center; }

.section-title p {
  font-size: 1.5rem;
  margin: 0;
  text-align: center; }

.list-filter {
  display: flex;
  justify-content: center;
  gap: 0.5rem;
  flex-wrap: wrap;
  margin: 1.5rem 0; }
  .list-filter .filter-item {
    font-size: 0.8rem;
    padding: 0.4rem 0.8rem;
    background: var(--dark);
    border-radius: 5px;
    cursor: pointer; }
    .list-filter .filter-item.active {
      background-color: var(--accent); }
    .list-filter .filter-item:hover {
      background-color: var(--accent); }

#search-query {
  padding: .8rem 1rem;
  background: var(--dark);
  border: 2px solid var(--dark);
  border-radius: 10px;
  outline: none;
  width: 100%;
  font-family: inherit;
  font-size: inherit;
  color: inherit; }
  #search-query:focus, #search-query:hover {
    border: 2px solid var(--accent); }

#search-results {
  display: none;
  margin-bottom: 1rem; }
  #search-results a {
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 0.5rem;
    font-size: 1.2rem; }
    #search-results a:hover {
      background-color: var(--tr-dark);
      border-radius: 5px; }
    #search-results a time {
      font-size: 1rem;
      font-weight: bold;
      color: var(--accent); }

.list-item {
  margin-bottom: 1rem; }
  .list-item h2 {
    font-size: 2rem;
    border-bottom: 4px solid var(--accent);
    margin: 0 0 1rem; }
  .list-item a {
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 0.5rem;
    font-size: 1.2rem; }
    .list-item a:hover {
      background-color: var(--tr-dark);
      border-radius: 5px; }
    .list-item a time {
      font-size: 1rem;
      font-weight: bold;
      color: var(--accent); }

.dashboard .chart {
  background-color: var(--tr-dark);
  border-radius: 10px;
  padding: 2rem; }

.notes {
  max-width: 1080px; }
  .notes h2 {
    font-size: 2rem;
    border-bottom: 4px solid var(--accent);
    margin: 0 0 1.5rem; }
  .notes .note-list {
    display: grid;
    grid-template-columns: auto auto auto;
    gap: 1.5rem; }
    .notes .note-list .note-item {
      border-radius: 10px;
      background-color: var(--darker);
      border: 4px solid var(--accent); }
      .notes .note-list .note-item .header {
        background-color: var(--accent);
        padding: 0.5rem 1rem; }
        .notes .note-list .note-item .header h3 {
          font-size: 1.4rem;
          margin: 0; }
        .notes .note-list .note-item .header .subtitle {
          font-size: 0.8rem;
          margin: 0; }
      .notes .note-list .note-item .description {
        display: flex;
        flex-direction: column; }
        .notes .note-list .note-item .description a {
          padding: 0.5rem 1rem; }
          .notes .note-list .note-item .description a h4 {
            font-size: 1.2rem;
            margin: 0; }
          .notes .note-list .note-item .description a p {
            margin: 0;
            font-size: 1rem; }
          .notes .note-list .note-item .description a:hover {
            background-color: var(--dark); }

