@font-face {
  font-family: "Jost";
  src: url("/fonts/Jost.ttf") format("truetype"); }

@font-face {
  font-family: "Roboto Mono";
  src: url("/fonts/RobotoMono.ttf") format("truetype"); }

:root {
  --light: #e6e6e6;
  --medium: #cccccc;
  --accent: #536DFE;
  --dark: #393E46;
  --darker: #222831;
  --tr-dark: #393E46ab;
  --nav-height: 8em;
  --font-size: 20px; }

* {
  box-sizing: border-box; }

html {
  font-family: "Jost";
  color: var(--light);
  font-size: 1rem;
  line-height: 1.5;
  scroll-behavior: smooth;
  background-color: var(--darker); }

header, footer {
  width: 100%; }

body {
  display: flex;
  flex-direction: column;
  margin: 0 auto; }

main {
  display: flex;
  flex-direction: column;
  align-items: center; }

section {
  max-width: 800px;
  width: 100%;
  padding-bottom: 2rem; }

a {
  text-decoration: none;
  color: var(--light); }

p,
ol,
ul,
dl,
table,
blockquote {
  font-size: 1.2rem;
  margin: 0 0 1.5rem 0;
  color: var(--medium); }

blockquote {
  padding: 1rem 0 1rem 1rem;
  border-left: 5px solid var(--dark); }
  blockquote p {
    margin: 0; }

table {
  border-collapse: separate;
  border-spacing: 0;
  margin-left: auto;
  margin-right: auto;
  overflow-x: auto; }

th {
  border-bottom: 4px solid var(--dark); }

td {
  border-bottom: 2px solid var(--dark); }

th, td {
  text-align: left;
  padding: 0.75rem 1.5rem;
  hyphens: auto; }

tr:hover {
  background-color: var(--dark); }

:not(pre) > code {
  font-size: 0.9rem;
  font-family: "Roboto Mono";
  padding: 3px 5px;
  border-radius: 4px;
  background: var(--dark); }

::marker {
  color: var(--accent); }

::selection {
  background: var(--accent);
  color: var(--light); }

::placeholder {
  color: var(--medium);
  font-family: "Jost"; }

/* Config */
/* mixins */
.animation {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: -1; }

.particle, .particle:after {
  background: transparent; }

.particle:after {
  position: absolute;
  content: "";
  top: 1920px; }

